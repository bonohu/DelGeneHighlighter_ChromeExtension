{
    "manifest_version": 3,
    "name": "DelGene Highlighter",
    "version": "1.0",
    "description": "Highlights keywords from a CSV file stored in the extension folder.",
    "permissions": ["activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["g2p_humanDec23_bonohu300.csv"],
        "matches": ["<all_urls>"]
      }
    ],
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      },
    "action": {
      "default_icon": "icon.png"
    }
  }
  
